{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/test_redis",
      "dest": "api/test_redis.py"
    },
    {
      "src": "/api/test_env",
      "dest": "api/test_env.py"
    },
    {
      "src": "/webhook",
      "dest": "api/webhook.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/webhook.py"
    }
  ]
}